.max_stack_size 1
.data
:float1 .word 1.5
:float2 .word -2.5

:add   .asciz "Add Int"
:sub   .asciz "Sub Int"
:mul   .asciz "Mul Int"
:div   .asciz "Div Int"
:rem   .asciz "Rem Int"
:and   .asciz "And Int"
:or   .asciz "Or Int"
:xor   .asciz "Xor Int"
:shl   .asciz "Shl Int"
:shr   .asciz "Shr Int"

:addf   .asciz "Add Float"
:subf   .asciz "Sub Float"
:mulf   .asciz "Mul Float"
:divf   .asciz "Div Float"
:remf   .asciz "Rem Float"

:eq     .asciz "Equal"
:ne     .asciz "Not Equal"

:ltu    .asciz "Less Than Unsigned"
:gtu    .asciz "Greater Than Unsigned"
:leu    .asciz "Less Than or Equal Unsigned"
:geu    .asciz "Greater Than Or Equal Unsigned"

:lts    .asciz "Less Than Signed"
:gts    .asciz "Greater Than Signed"
:les    .asciz "Less Than or Equal Signed"
:ges    .asciz "Greater Than Or Equal Signed"


:ltf    .asciz "Less Than Float"
:gtf    .asciz "Greater Than Float"
:lef    .asciz "Less Than or Equal Float"
:gef    .asciz "Greater Than Or Equal Float"




.text
LDI W0 0
LDI W4 1
LDI W2 3 ; 0b011
LDI W3 5 ; 0b101         0110  1111 1001

PCR W1 #add
SYS 3 ; print string
ADD W1 W2 W3
SYS 1

PCR W1 #sub
SYS 3 ; print string
SUB W1 W3 W2 
SYS 1

PCR W1 #mul
SYS 3 ; print string
MUL W1 W2 W3 
SYS 1

PCR W1 #div
SYS 3 ; print string
DIV W1 W3 W2
SYS 1

PCR W1 #rem
SYS 3 ; print string
REM W1 W2 W3
SYS 1

PCR W1 #and
SYS 3 ; print string
AND W1 W2 W3 
SYS 1

PCR W1 #or
SYS 3 ; print string
OR W1 W2 W3 
SYS 1

PCR W1 #xor
SYS 3 ; print string
XOR W1 W2 W3 
SYS 1
XORI W1 W1 -1
SYS 1

PCR W1 #shl
SYS 3 ; print string
SHL W1 W2 W3 
SYS 1

PCR W1 #shr
SYS 3 ; print string
SHR W1 W2 W4
SYS 1

PCR W2 #float1
LDA W2 W2 0
PCR W3 #float2 
LDA W3 W3 0

PCR W1 #addf
SYS 3 ; print string
ADDF W1 W2 W3
SYS 2

PCR W1 #subf
SYS 3 ; print string
SUBF W1 W2 W3
SYS 2

PCR W1 #mulf
SYS 3 ; print string
MULF W1 W2 W3
SYS 2

PCR W1 #divf
SYS 3 ; print string
DIVF W1 W2 W3
SYS 2


PCR W1 #remf
SYS 3 ; print string
REMF W1 W2 W3
SYS 2

;;;;;;;;;;;;;;;;;;;;;;;;;;
;; Unsigned Comparison ;;
;;;;;;;;;;;;;;;;;;;;;;;;;

LDI W2 3 ; 0b011
LDI W3 5 ; 0b101         0110  1111 1001

PCR W1 #eq
SYS 3
EQ W1 W2 W3
SYS 1

PCR W1 #ne
SYS 3
NE W1 W2 W3
SYS 1

PCR W1 #ltu
SYS 3
LTU W1 W2 W3
SYS 1

PCR W1 #gtu
SYS 3
GTU W1 W2 W3
SYS 1

PCR W1 #leu
SYS 3
LEU W1 W2 W3
SYS 1

PCR W1 #geu
SYS 3
GEU W1 W2 W3
SYS 1


;;;;;;;;;;;;;;;;;;;;;;;
;; Signed Comparison ;;
;;;;;;;;;;;;;;;;;;;;;;;

LDI W2 3 ; 0b011
LDI W3 -5 ; 0b101         0110  1111 1001

PCR W1 #lts
SYS 3
LTS W1 W2 W3
SYS 1

PCR W1 #gts
SYS 3
GTS W1 W2 W3
SYS 1

PCR W1 #les
SYS 3
LES W1 W2 W3
SYS 1

PCR W1 #ges
SYS 3
GES W1 W2 W3
SYS 1

;;;;;;;;;;;;;;;;;;;;;;
;; Float Comparison ;;
;;;;;;;;;;;;;;;;;;;;;;

PCR W2 #float1
LDA W2 W2 0
PCR W3 #float2
LDA W3 W3 0

PCR W1 #ltf
SYS 3
LTF W1 W2 W3
SYS 1

PCR W1 #gtf
SYS 3
GTF W1 W2 W3
SYS 1

PCR W1 #lef
SYS 3
LEF W1 W2 W3
SYS 1

PCR W1 #gef
SYS 3
GEF W1 W2 W3
SYS 1

PCR W1 #eq
SYS 3
EQ W1 W2 W3
SYS 1


SYS 0